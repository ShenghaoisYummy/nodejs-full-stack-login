{
  "version": 2,
  "buildCommand": "cd 'Nodejs login /frontend' && npm install && npm run build",
  "outputDirectory": "Nodejs login /frontend/dist",
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "headers": {
        "cache-control": "s-maxage=0"
      }
    }
  ]
}
