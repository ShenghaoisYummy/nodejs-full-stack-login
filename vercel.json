{
  "version": 2,
  "builds": [
    {
      "src": "Nodejs login /frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/*.js",
      "use": "@vercel/node@3.0.5"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Nodejs login /frontend/dist/$1"
    }
  ]
}
