{
  "version": 2,
  "buildCommand": "cd 'Nodejs login /frontend' && npm install && npm run build",
  "outputDirectory": "Nodejs login /frontend/dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/login.js": {
      "runtime": "nodejs18.x"
    },
    "api/register.js": {
      "runtime": "nodejs18.x"
    },
    "api/profile.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
